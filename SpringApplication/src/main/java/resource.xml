<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans.xsd
  http://www.springframework.org/schema/context
  http://www.springframework.org/schema/context/spring-context.xsd">

<context:annotation-config/>

<bean id="myadd" class="com.tns.annotations.Address">
	<constructor-arg value="Nashik"></constructor-arg>
	<constructor-arg value="Maha"></constructor-arg>
	<constructor-arg value="India"></constructor-arg>
	<constructor-arg value="12345"></constructor-arg>
</bean>

<bean id="res_employee" class="com.tns.annotations.Employee">
<constructor-arg value="Vinaya"></constructor-arg>
<constructor-arg value="100"></constructor-arg>

</bean>
</beans>